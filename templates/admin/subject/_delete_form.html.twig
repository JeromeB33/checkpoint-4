<form method="post" action="{{ path('dashboard_subject_delete', {'id': subject.id}) }}" onsubmit="return confirm('Êtes-vous sûr de vouloir supprimer cet subject ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ subject.id) }}">
    <button class="btn">Supprimer</button>
</form>
